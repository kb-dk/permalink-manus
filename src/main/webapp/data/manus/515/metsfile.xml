            <md:title>Augustinus, De civitate dei</md:title>
          </md:titleInfo>
          <md:titleInfo xml:lang="eng">
            <md:title>Augustinus, De civitate dei</md:title>
          </md:titleInfo>
          <md:name xml:lang="dan">
            <md:partName>Fragm. 3133</md:partName>
            <md:role>
              <md:roleTerm>author</md:roleTerm>
            </md:role>
          </md:name>
          <md:name xml:lang="eng">
            <md:partName>Fragm. 3133</md:partName>
